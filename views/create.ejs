
<%- include("./partials/header_event_create.ejs") %>


  <div class="create-event content">
    <h2>Create Event</h2>
    <form action="/events" method="POST">
      <label for="title">Event title:</label>
      <input type="text" id="title" name="title" required>
      <label for="description">Event description:</label>
      <input type="text" id="description" name="description" required>
      <label for="location">Event location:</label>
      <input type="text" id="location" name="location" required>
      <label for="timings">Event timings:</label>
      <input id="timings" name="timings" required></input>
      <label for="register">Registration Link:</label>
      <input id="register" name="register" required></input>
      <button>Submit</button>
    </form>
  </div>

  <!-- <%- include("./partials/footer.ejs") %> -->
</body>
</html>